{
  "name": "roomcasino",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt --port 8080",
    "build": "NUXT_ENV_MODE=production nuxt build",
    "build:sandbox": "NUXT_ENV_MODE=sandbox nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "npm run lint:js && npm run lint:css",
    "lintfix": "eslint --fix --ext .js,.vue . && stylelint '**/*.{scss,vue}' --fix",
    "lint:js": "eslint --ext .js,.vue .",
    "lint:css": "stylelint '**/*.{scss, vue}'"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "cookieparser": "^0.1.0",
    "core-js": "^3.6.5",
    "js-cookie": "^2.2.1",
    "moment": "^2.29.1",
    "nuxt": "^2.14.6",
    "nuxt-start": "^2.14.7",
    "v-click-outside": "^3.1.1",
    "vue-js-modal": "^2.0.0-rc.6",
    "vue-marquee-text-component": "^1.1.1",
    "vue-slider-component": "^3.2.10",
    "vuejs-datepicker": "^1.6.2",
    "vuelidate": "^0.7.5"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.7",
    "@nuxtjs/style-resources": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-nuxt": "^1.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-prettier-vue": "^2.1.1",
    "eslint-plugin-vue": "^7.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "prettier": "^1.19.1",
    "sass": "^1.28.0",
    "sass-loader": "^10.0.4",
    "stylelint": "^13.8.0",
    "stylelint-order": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "npm run lintfix",
      "git add"
    ],
    "*.js": "eslint --cache --fix",
    "*.vue": "stylelint --fix"
  },
  "browserslist": [
    "defaults",
    "last 2 safari version"
  ]
}
